<%= form_with url: "/logout", method: :post, class: "form-style-6" do |form| %>
	<% if !flash.empty? %>
				<div id="flash"> 
						<% flash.keys.each do |k| %> 
								<div class="<%= k %>">
										<%= flash[k] %>
								</div>  
						<% end %>   
				</div>
	<% else %>
		<p> SUCCESS!! </p>
	<% end %>
	<%= form.submit "Ok" %>
<% end %>